{
  "rules": {
    "eofline": true,
    "indent": true,
    "radix": true,
    "no-arg": true,
    "curly": true,
    "no-duplicate-variable": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-use-before-declare": false,
    "one-line": [true, "check-catch", "check-else"],
    "semicolon": true,
    "no-empty": true,
    "class-name": true,

    "no-any": false,
    "variable-name": false
  }
}
